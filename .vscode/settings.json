{
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.o": true,
    "build_*": true,
    "devel_*": true,
    "logs_*": true,
    ".vscode/ipch/*": true,
    "**/.git": true,
    "vision_logs/*": true,
    "vive_logs/*": true,
    ".deploy/*": true,
    "monitoring/*": true
  },
  "catch2TestExplorer.executables": [
    "./devel_debug/.private/**"
  ],
  "files.associations": {
    "functional": "cpp",
